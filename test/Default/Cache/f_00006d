<script type="text/javascript">
		//Load Mbox Plugin for SC
		// Enable the plugin only if the flag namely, enableMboxLoadSCPlugin is enabled
		if (enableMboxLoadSCPlugin) mboxLoadSCPlugin(s);

	if (typeof vzwSc != "undefined" && vzwSc) {



		var scErrorOccurred = false;
		var pName = "";
		if (pName != "") {
			vzwSc.setValue("Channel", "/store");
		}
		vzwSc.setPageName(pName);

		var dataExceptionPName = "false";
		var unavailablePName = "true";
		if (dataExceptionPName == "true") {
			vzwSc.setPageNameNoChannel(vzwSc.previousPage()+ " data exception");
		} else if (unavailablePName == "true") {
			vzwSc.setPageNameNoChannel(vzwSc.previousPage()+ " unavailable");
		}

		vzwSc.setValue("PageType", "vzw");
		vzwSc.setValue("Application", "vzw");
		vzwSc.setValue("globalId", "owJOu26RAsl8i%2By7MopKC5hR3vzGAF%2F6QptplcNNqZI2LCZW8nkzf9q2jTMCusEM");

		var errorPType = "errorPage";
		if (errorPType == "errorPage") {
			vzwSc.setValue("ErrorPageType", "errorPage");
		}

		var checkOut = "false";
		if (checkOut == "true") {
			vzwSc.setValue("Events", "scCheckout");
		}

		var purchase = "";
		if (purchase == "true"){
			vzwSc.setValue("Events","purchase");
		}

		var purcaseRelatedEvents = "";
		if (purcaseRelatedEvents == "true"){
			vzwSc.setValue("Events","event22,event23,event26,event27");
		}

		var reportSuite = "";
		if (reportSuite == "vzwibusiness"){
			vzwSc.setRsBusinessFlag("true");
		}

		vzwSc.setValue("impersonator", "");
		vzwSc.setValue("customerType", "b2c");
		vzwSc.setValue("ShippingMethod", "");
		vzwSc.setValue("PaymentMethod", "");
		vzwSc.setValue("PaymentType", "");
		vzwSc.setValue("BillingState", "");
		vzwSc.setValue("BillingZip", "");
		vzwSc.setValue("PromoCode", "");
		vzwSc.setValue("OrderType", "");
		vzwSc.setValue("Email", "");
		vzwSc.setValue("OrderNumber","");
		vzwSc.setValue("AlltelMsg", "");

		//ISPU
		var ispuAlreadySet="";
		if (ispuAlreadySet == null || ispuAlreadySet == "") {
			var ispuActive="";
				if (ispuActive == "true") {
					var storeNum="";
					if (storeNum != null || storeNum != "") {
						vzwSc.setValue("StoreNumber", "");
					}
				}
		} else if (ispuAlreadySet == "blank") {
			vzwSc.setValue("StoreNumber", "");
		} else {
			vzwSc.setValue("StoreNumber", "");
		}


		//store name on specific pages
		var tagStoreName = "no";
		if (tagStoreName == "true") {
			vzwSc.setValue("StoreName", "b2c");
		}
		//Zip, Area and Region
		var tagZipCode = "false";
		var tagAreaRegion = "false";

		var zip = "";
		var area = "";

		if (zip == null || zip == "") {
			zip = "";

		}
		area = area.concat(vzwSc.replace(""))
				.concat("|")
			 	.concat(vzwSc.replace(""));

		//Tag Zip and Area
		if (zip != null && zip.length > 0 && tagZipCode) {
			vzwSc.setValue("MarketZip", zip);
		}

		if (area.length > 1 && tagAreaRegion) {
			vzwSc.setValue("AreaRegion", area);
		}

		var isStoreLocatorSearch = "none";
		if (isStoreLocatorSearch == "slByZip") {
			var searchZipCode = "";
			if (searchZipCode != null && searchZipCode != "null" && searchZipCode != "") {
				vzwSc.setValue(isStoreLocatorSearch, searchZipCode);
			}
		}

		if (isStoreLocatorSearch == "slByState") {
			var searchStateName = "";
			if (searchStateName != null && searchStateName != "null" && searchStateName != "") {
				vzwSc.setValue(isStoreLocatorSearch, searchStateName);
			}
		}
		if (s.pageName == "content category/put page name here") {
			//alert(hbx.pn +"    :     "+hbx.mlc      +"-JSP");
			//Set pageName
			if (typeof hbx.pn != "undefined" && hbx.mlc != "undefined") {
				s.pageName = hbx.mlc.toLowerCase() + "/" + hbx.pn.toLowerCase();
				var pn_hold = new Array();
				pn_hold = s.pageName.split('+');
				s.pageName = pn_hold.join(' ');
			} else {
				s.pageName = document.location.pathname.toLowerCase()
			}
			s.pageName = s.pageName.split('//').join('/');
			//if( typeof hbx.pn != "undefined" && hbx.pn != "CONTENT+CATEGORY/PUT+PAGE+NAME+HERE"){
			//  s.pageName = hbx.mlc.toLowerCase() + "/" + hbx.pn.toLowerCase();
			//  var pn_hold = new Array();
			//  pn_hold = s.pageName.split('+');
			//  s.pageName = pn_hold.join(' ');
			//}else{
			//s.pageName = document.location.pathname.toLowerCase()
			// }
		}

		//Event that is called every time the stopping cart is opened
		var setCartOpensEvent = "false"
		if (setCartOpensEvent == "true") {
			vzwSc.setValue("Events","scOpen");
		}

		//Event that is called every time the stopping cart has an item added
		var setCartAddEvent = "false"
		if (setCartAddEvent == "true") {
			vzwSc.setValue("Events","scAdd");
		}

		//Event sets the add and remove values in the SC
		var scAR = "";
		if (scAR == "scAdd") {
			vzwSc.setValue("Events", "scAdd");
		} else if(scAR == "scRemove") {
			vzwSc.setValue("Events", "scRemove");
		} else if(scAR == "scView") {
			vzwSc.setValue("Events", "scView");
		}

		/*************  SC Flow related logic Begin ******************/
		var scFlow = "true";
		var sc_FlowState = null;
		var sc_FlowType = null;
		var sc_FlowName = null;
		var sc_FlowInteraction = null;
		var sc_FlowTypeFlag = "true";
		var sc_FlowInteractionFlag = "true";
		var nm = vzwSc.replace("");
		if (scFlow == "true") {
			if ( nm != null && nm != "null" && nm != "" ) {
				nm = "/" + "vzw" + nm;
				nm = nm.toLowerCase();
				var channelVars=nm.split("/");
				sc_FlowState = "empty";
				sc_FlowName = "/" + channelVars[2];
				if( sc_FlowTypeFlag == "true"){
					if ( channelVars.length-1 >= 3 &&
							(nm.indexOf("/aal/existing fs") != -1 ||
								nm.indexOf("/aal/convert fs") != -1 ||
								nm.indexOf("/aal/individual plan") != -1 ||
								nm.indexOf("/aal/shopping assistant") != -1 ||
								nm.indexOf("/aal/select phone or device") != -1 ||
								nm.indexOf("/aal/shopping assitant") != -1 ||
								nm.indexOf("/aal/nso") != -1 ||
								nm.indexOf("/upgrade") != -1 ||
								nm.indexOf("/employee discounts") != -1 ||
								nm.indexOf("/activate device") != -1 ||
								nm.indexOf("/activate phone") != -1 ||
								nm.indexOf("/activate or switch device") != -1 ||
								(nm.indexOf("/change share all plan") != -1 && nm.indexOf('/billing options next bill') != -1) ||
								(nm.indexOf("/change share all plan") != -1 && nm.indexOf('/review changes next bill') != -1))){
						sc_FlowType = "/" + channelVars[2] +"/" + channelVars[3];
					} else {
						if( sc_FlowName.indexOf("/employee discounts") != -1 ||
								sc_FlowName.indexOf("/prepay") != -1){
							sc_FlowType = "/" + channelVars[2] + "/" + channelVars[3];
						} else {
							sc_FlowType = "/" + channelVars[2];
						}
					}
				}
				if( sc_FlowInteractionFlag == "true"){
					sc_FlowInteraction = "";
					if( sc_FlowInteraction != "" ){
						if ( channelVars.length-1 >= 3 &&
								(nm.indexOf("/aal/existing fs") != -1 ||
										nm.indexOf("/aal/convert fs") != -1 ||
										nm.indexOf("/aal/individual plan") != -1 ||
										nm.indexOf("/aal/shopping assistant") != -1 ||
										nm.indexOf("/aal/select phone or device") != -1 ||
										nm.indexOf("/aal/shopping assitant") != -1 ||
										nm.indexOf("/aal/nso") != -1 ||
										nm.indexOf("/upgrade") != -1 )){
								sc_FlowInteraction = "/" + channelVars[2] + "/" + channelVars[3] + "/" + sc_FlowInteraction;
						} else {
							sc_FlowInteraction = "/" + channelVars[2] + "/" + sc_FlowInteraction;
						}
					} else {
						if ( channelVars.length-1 >= 3 &&
								(nm.indexOf("/aal/existing fs") != -1 ||
										nm.indexOf("/aal/convert fs") != -1 ||
										nm.indexOf("/aal/individual plan") != -1 ||
										nm.indexOf("/aal/shopping assistant") != -1 ||
										nm.indexOf("/aal/select phone or device") != -1 ||
										nm.indexOf("/aal/nso") != -1 ||
										nm.indexOf("/aal/shopping assitant") != -1 ||
										nm.indexOf("/upgrade") != -1 ||
										nm.indexOf("/prepay") != -1)){
								sc_FlowInteraction = "/" + channelVars[2] + "/" + channelVars[3] + "/" + channelVars[channelVars.length-1];
						} else {
							sc_FlowInteraction = "/" + channelVars[2] + "/" + channelVars[channelVars.length-1];
						}

						if(pName=="/verizon wireless sitemap"){
							sc_FlowInteraction ="/verizon wireless sitemap";
						}else if(pName=="/footer/legal notices" || pName=="/gift card" || pName=="/refill card"){
							sc_FlowInteraction = "/" + channelVars[2];
						}else if(pName=="/lifeline/landing page"){
							sc_FlowInteraction = "/vzw" + "/" + channelVars[2] + "/landing page";
						}else if(pName=="/lifeline/application"){
							sc_FlowInteraction ="/vzw" + "/" + channelVars[2] + "/application";
						}
					}
				}
				vzwSc.setProcessFlowMetrics(sc_FlowState, sc_FlowName, sc_FlowType, sc_FlowInteraction);
			}
		}

		//Product View
		var sc_ProdView = "false";
		if (sc_ProdView == "true") {
			var sc_Cat = "";
			var sc_Sku = "";
			var sc_Qty = "";
			var sc_Price = "";

			var products = vzwSc.buildProductsViewString("", sc_Cat, sc_Sku, sc_Qty, sc_Price);
			vzwSc.setProductView(products);

		}

		//product view if string is ready to pass directly
		var sc_SetProductsString = "false";
		if (sc_SetProductsString == "true"){
			var sc_ProductsString = "";
			//following if statement used to verify that there is no comma at the end of the string, if there is a comma the final product values will be duplicated
			if (sc_ProductsString.charAt(sc_ProductsString.length-1) == ",")
				sc_ProductsString = sc_ProductsString.slice(0,-1);
			vzwSc.setProductView(sc_ProductsString);
		}

		//set logged in status
		var sc_CustomerProspectInd = "prospect";
		var status="Unauthenticated";



		vzwSc.setValue("CustomerProspectInd", sc_CustomerProspectInd);
		vzwSc.setValue("status", status);

		//set ECP ID and Account Number
		var sc_setAccountNumber = "";
		if (sc_setAccountNumber == 'true') {
			var sc_AccountNumber = "";
			var ecpdId="";
			if(ecpdId !=0){
				var sc_ECPDNumber = "";
			} else {
				ecpdId="";
				if(ecpdId !=0){
					var sc_ECPDNumber = "";
				}
			}
			vzwSc.setValue("AccountID", sc_AccountNumber);
			vzwSc.setValue("EcpID",sc_ECPDNumber);
		}

		//Error Displayed to Customer
		var errorDisplayed = "true";
		var errorCvC6 = '';
		if (errorDisplayed == "true" || errorCvC6 !="") {
			var errorMsg = 'Page is unavailable';
			if (errorCvC6 != "") {
				vzwSc.setValue("error" , vzwSc._stripIllegalChars(errorCvC6,1));
			} else{
				vzwSc.setValue("error", errorMsg);
			}
			scErrorOccurred = true;
		}

		//This code will check the products string for the event "event26" (activation fee), if that event exists in prod string then also throw it in the events(if it is not already there)
		function scActivationFeeEventThrower(){
			var scTempString = "";
			if(scTempString != ""){
				if(scTempString.indexOf("event26") != -1){
					vzwSc.setValue("Events", "event26");
				}
			}
		}
		scActivationFeeEventThrower();

	    vzwSc.setGlobalIdForOverlayPages();
	    var sc_submitData = "true";
	    if (sc_submitData == "true") {
			vzwSc.submitData(scErrorOccurred);
	    }
	}



</script>
